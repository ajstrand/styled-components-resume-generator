{
  "parser": "babel-eslint",
  "plugins":["jsx-a11y", "jest-dom", "testing-library"],
  "extends": [
    "plugin:prettier/recommended",
    "preact"
  ],
  "env": {
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 2020,
    "ecmaFeatures": {
      "legacyDecorators": true,
      "jsx": true
    }
  },
  "rules": {
    "space-before-function-paren": 0,
    "import/export": 0,
    "no-unused-vars":"warn"
  }
}

