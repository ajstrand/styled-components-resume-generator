{
  "name": "styled-resume",
  "version": "0.0.0",
  "scripts": {
    "start": "npm run dev",
    "dev": "node ./server",
    "prod": "npm run build && npm run prod:serve",
    "// Build for production": "",
    "build": "npm run build:clean && npm run build:vite && npm run build:prerender",
    "build:clean": "trash dist/",
    "build:vite": "vite build && vite build --ssr",
    "build:prerender": "vite-plugin-ssr prerender",
    "// Static serve for production": "",
    "prod:serve": "serve dist/client/ --listen 3000",
    "dev-old": "vite", 
    "build-old": "vite build",
    "serve": "vite preview"
  },
  "dependencies": {
    "react":"17.0.2",
    "preact": "^10.5.14",
    "prop-types": "^15.7.2"
  },
  "devDependencies": {
    "@babel/plugin-transform-react-jsx": "^7.14.9",
    "@callstack/react-theme-provider": "^3.0.6",
    "@linaria/babel-preset": "^3.0.0-beta.13",
    "@linaria/core": "^3.0.0-beta.13",
    "@linaria/react": "^3.0.0-beta.13",
    "@linaria/rollup": "^3.0.0-beta.13",
    "@linaria/shaker": "^3.0.0-beta.13",
    "@preact/preset-vite": "^2.1.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/preact": "^2.0.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-macros": "^3.1.0",
    "babel-plugin-require-context-hook": "^1.0.0",
    "babel-plugin-styled-components": "^1.12.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "core-js": "^3.17.3",
    "cross-env": "^7.0.3",
    "css-loader": "^4.3.0",
    "eslint": "^7.5.0",
    "express": "^4.17.1",
    "gh-pages": "^3.2.3",
    "husky": "^4.3.6",
    "jest": "^27.2.0",
    "jest-cli": "^27.2.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.6",
    "preact-iso": "^2.2.0",
    "preact-render-to-string": "^5.1.19",
    "pretty-quick": "^3.1.1",
    "styled-components": "^5.2.1",
    "stylelint": "^13.13.1",
    "stylelint-config-recommended": "^5.0.0",
    "vite": "^2.5.7",
    "vite-plugin-linaria": "^1.0.0",
    "vite-plugin-ssr": "^0.3.1"
  }
}
