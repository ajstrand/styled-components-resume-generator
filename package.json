{
  "name": "styled-resume",
  "version": "0.0.1",
  "description": "generator that creates a resume with styled components, html and css grid",
  "author": "ajstrand",
  "license": "MIT",
  "repository": "ajstrand/styled-resume-generator",
  "main": "dist/styled-resume.js",
  "module": "dist/styled-resume.es.js",
  "jsnext:main": "dist/styled-resume.es.js",
  "engines": {
    "node": ">=10",
    "npm": ">=5"
  },
  "scripts": {
    "test:generate-output": "jest --json --outputFile=.jest-test-results.json || true",
    "test": "jest",
    "test:watch": "react-scripts-ts test --env=jsdom",
    "build": "tsc --outDir build/lib --declarationDir build/types --declaration true && rollup -c",
    "start": "rollup -c -w",
    "prepare": "npm run build",
    "predeploy": "cd example && npm install && npm run build",
    "deploy": "gh-pages -d example/build",
    "lint:css": "stylelint src/**/*.tsx"
  },
  "dependencies": {
    "babel-plugin-polished": "^1.1.0",
    "core-js": "^3.8.1",
    "polished": "^4.1.1",
    "prop-types": "^15.7.2",
    "regenerator-runtime": "^0.13.7"
  },
  "peerDependencies": {
    "preact": "10.5.13"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.2",
    "preact": "^10.5.13",
    "@types/lodash": "^4.14.166",
    "preact-iso": "^1.1.1",
    "@babel/core": "^7.13.16",
    "@babel/plugin-proposal-export-default-from": "^7.12.1",
    "@babel/plugin-transform-react-jsx": "^7.13.12",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-html": "^0.2.3",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@rollup/plugin-replace": "^2.4.2",
    "@rollup/plugin-typescript": "^8.1.0",
    "@rollup/plugin-url": "^6.0.0",
    "@svgr/rollup": "^5.5.0",
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/preact": "^2.0.1",
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.16",
    "@types/styled-components": "^5.1.9",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-macros": "^3.0.1",
    "babel-plugin-require-context-hook": "^1.0.0",
    "babel-plugin-styled-components": "^1.12.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "cross-env": "^7.0.3",
    "css-loader": "^4.3.0",
    "eslint": "^7.24.0",
    "eslint-config-preact": "^1.1.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "fork-ts-checker-webpack-plugin": "^6.0.8",
    "gh-pages": "^3.1.0",
    "husky": "^4.3.6",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "jest-styled-components": "^7.0.3",
    "node-sass": "^4.14.1",
    "prettier": "2.2.1",
    "pretty-quick": "^2.0.2",
    "react-docgen-typescript-loader": "^3.7.2",
    "require-context.macro": "^1.2.2",
    "rollup": "^2.45.2",
    "rollup-plugin-dev": "^1.1.3",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-scss": "^2.6.1",
    "rollup-plugin-serve": "^1.1.0",
    "sass-loader": "^10.1.0",
    "style-loader": "^2.0.0",
    "styled-components": "^5.2.1",
    "styled-theming": "^2.2.0",
    "stylelint": "^13.12.0",
    "stylelint-config-recommended": "^4.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "ts-jest": "^26.5.5",
    "ts-loader": "^8.0.12",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4",
    "webpack": "^4.44.2"
  },
  "files": [
    "dist"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
